{
    "version": 2,
  "outputDirectory": "./build",
  "builds": [
    {
        "src": "./index.js",
        "use": "@vercel/node",
         "config": { "includeFiles": ["build/**"] }
    }
   ],
   "routes": [
        {
            "src": "/(.*)",
            "dest": "build/index.js"
        }
    ]
}